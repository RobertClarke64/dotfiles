urxvt*borderColor:  [95]#2e3440
! urxvt*background:	rgba:0000/0000/0000/dddd
urxvt*foreground: 	#FFFFFF

urxvt*externalBorder:	0
urxvt*internalBorder:	2
urxvt*scrollBar:	false
! urxvt*geometry:		136x45
