Xft.dpi:			96
Xft.antialias:		true
Xft.rgba:			rgb
Xft.hinting:		true
Xft.hintstyle:		hintslight

urxvt*cursor:		Bibata Ice
urxvt*depth:		32
! urxvt*background: 	[85]#111111
urxvt*background: 	[95]#2e3440
urxvt*foreground: 	#FFFFFF
urxvt*scrollBar:	false
urxvt*geometry:		150x38
urxvt*font:			xft:Inconsolata:size=13
! urxvt*font:		xft:Dejavu Sans Mono:size=11
!urxvt*font:		xft:Spleen:style=regular:size=13
!urxvt*font:		xft:SF Pro Display:style=regular:size=11
urxvt*letterSpace:	0
urxvt*perl-ext-common:	default,matcher,selection-to-clipboard,new-window
urxvt*pointerBlank:	true

! urxvt.keysym.C-N: perl:new-window

! black
URxvt.color0  : #000000
URxvt.color8  : #555555
! red
! URxvt.color1  : #AA3333
URxvt.color1  : #CC3333
URxvt.color9  : #FF5555
! green
URxvt.color2  : #00AA00
URxvt.color10 : #55FF55
! yellow
URxvt.color3  : #f4c141
URxvt.color11 : #FFFF55
! blue
URxvt.color4  : #0000AA
URxvt.color12 : #5555FF
! magenta
URxvt.color5  : #AA00AA
URxvt.color13 : #FF55FF
! cyan
URxvt.color6  : #00AAAA
URxvt.color14 : #55FFFF
! white
URxvt.color7  : #AAAAAA
URxvt.color15 : #FFFFFF
