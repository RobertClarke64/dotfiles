Xft.dpi:			96
Xft.antialias:		true
Xft.rgba:			rgb
Xft.hinting:		true
Xft.hintstyle:		hintslight

urxvt*cursor:		Bibata Ice

urxvt*depth:		32
! urxvt*background: 	[85]#111111
